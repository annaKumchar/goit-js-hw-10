<ul class='country-info__list'>
  <li class='country-info__item'><p class='country-info__text'><b>Capital:
      </b>{{capital}}</p></li>
  <li class='country-info__item'><p class='country-info__text'><b>Population:
      </b>{{population}}</p></li>
  <li class='country-info__item'><p class='country-info__text'><b>Languages:
      </b>{{#each languages}}{{#if @index}}, {{/if}}<span
        >{{this}}</span>{{/each}}</p></li>
</ul>